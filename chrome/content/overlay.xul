<?xml version="1.0"?><?xml-stylesheet href="chrome://htlivesight/content/overlay.css" type="text/css"?><?xml-stylesheet href="chrome://htlivesight/content/button/button.css" type="text/css"?><!DOCTYPE window SYSTEM "chrome://htlivesight/locale/overlay.dtd"><overlay id="htlivesight_overlay"         xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"><!-- added loader part for adding button on install  -->        <script type="application/x-javascript" src="./prefs.js" /><!-- <script type="application/x-javascript" src="./preferences.js" /> -->   <!-- end added loader part for adding button on install --><script type="application/x-javascript" src="overlay.js" /><script type="application/x-javascript" src="log.js" /><script type="application/x-javascript" src="./button/button.js" /><!-- install button added by bigpapy --><!-- load listeners --><script type="application/x-javascript"><![CDATA[	window.addEventListener("load",	function() {//		alert("browserLoad");		//	alert("browserLoad1");//	htlivesight.prefs = htlivesight.Preferences.get();//	alert("browserLoad2");	// to add FoxTrick button on the navigation bar by default	HtlivesightPrefs.init();	if (!HtlivesightPrefs.getBool("HtlsToolbarInited")) {		var buttonId = "htls-button-1"; // ID of FoxTrick button		var afterId = "search-container"; // insert after search box		var navBar = document.getElementById("nav-bar");		var curSet = navBar.currentSet.split(",");//		alert("browserLoad3");		if (curSet.indexOf(buttonId) == -1) {//			alert("browserLoad3.1");			var pos = curSet.indexOf(afterId) + 1 || curSet.length;//			alert("browserLoad3.2");			var set = curSet.slice(0, pos).concat(buttonId).concat(curSet.slice(pos));//			alert("browserLoad4");			navBar.setAttribute("currentset", set.join(","));			navBar.currentSet = set.join(",");			document.persist(navBar.id, "currentset");//			alert("browserLoad5");			try {//			alert("browserLoad5.1");				BrowserToolboxCustomizeDone(true);//				alert("browserLoad5.2");			}			catch (e) {}		}//		alert("browserLoad5.3");//		htlivesight.prefs.general.toolbarInited= true;//		htlivesight.Preferences.save(htlivesight.prefs);		HtlivesightPrefs.setBool("HtlsToolbarInited",true);//		alert("browserLoad6");	};			//		htlivesight.browserLoad(document);//		alert("browserLoadEnd");	}, false);]]></script><!-- end added part for install button --><!-- Firefox --><toolbarpalette id="BrowserToolbarPalette">  <toolbarbutton id="htls-button-1"/>  </toolbarpalette><!-- button details --><toolbarbutton id="htls-button-1"  insertbefore="print-button"  label="HTLiveSight"  tooltiptext="Click to open HTLiveSight"  class="toolbarbutton-1"  type="menu"  context="HTLS-menu">  <menupopup id="HTLS-menu">		<menuitem id="Preferences" label="Options" oncommand="htls[3]();"/>		<menuitem id="Start HTLS" label="Start" oncommand="htls[2]();" />	</menupopup></toolbarbutton>  <menupopup id="menu_ToolsPopup">   <menu id="hattricktools_menu" label="Hattrick">    <menupopup id="hattricktools_menupopup">      <menuitem id="htlivesight_menuitem"      label="&menuitem.value;"      oncommand="htlivesight.onMenuItemCommand(event);" />    </menupopup>  </menu></menupopup></overlay>