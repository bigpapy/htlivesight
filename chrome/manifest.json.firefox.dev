{  
   "manifest_version":2,
   "name":"HTLiveSightDEV",
   "version":"0.9.883",
   "description":"A Hattrick Live Match Viewer",
   "icons":{  
      "16":"skin/16dev.png",
      "48":"skin/48dev.png",
      "128":"skin/128dev.png"
   },
   "browser_action":{  
      "default_icon":{
		"16":"skin/16dev.png",
		"24":"skin/24dev.png",
		"32":"skin/32dev.png",
		"48":"skin/48dev.png"
	  },
      "default_title":"Launch HTLiveSightDEV"
   },
   "background":{  
      "scripts":[  
         "content/button/startButton.js"
      ],
      "persistent":false
   },
   "homepage_url":"http://htlivesight.sourceforge.net",
   "incognito":"spanning",
   "minimum_chrome_version":"13",
   "permissions":[  
      "contextMenus",
      "notifications",
      "clipboardWrite",
      "*://chpp.hattrick.org/*"
   ],
   "options_ui":{  
      "page":"content/settings.html",
      "chrome_style":false,
      "open_in_tab":true
   },
   "web_accessible_resources":[  
      "content/img/foxball_32.png",
      "content/old_img/foxball_32.png",
      "content/themes/images/bg.png",
      "skin/128dev.png"
   ],
   "applications":{  
      "gecko":{  
         "update_url":"https://htlivesight.c6.ixsecure.com/dev/update.json"
      }
   }
}