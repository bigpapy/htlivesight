{  
   "manifest_version":3,
   "name":"HTLiveSightDEV",
   "version":"0.9.928",
   "description":"A Hattrick Live Match Viewer",
   "icons":{  
      "16":"skin/16dev.png",
      "48":"skin/48dev.png",
      "128":"skin/128dev.png"
   },
   "action":{  
      "default_icon":"skin/16dev.png",
      "default_title":"Launch HTLiveSight!"
   },
   "background":{  
      "service_worker":"content/button/startButton.js"
   },
   "homepage_url":"http://htlivesight.sourceforge.net",
   "incognito":"spanning",
   "minimum_chrome_version":"88",
   "options_page":"content/settings.html",
   "permissions":[  
      "notifications",
      "clipboardWrite"
   ],
   "host_permissions": [
      "*://chpp.hattrick.org/*"
  ],
   "web_accessible_resources":[
   {
	  "resources":[
		  "content/img/foxball_32.png",
		  "content/old_img/foxball_32.png",
		  "content/themes/images/bg.png",
		  "skin/128.png",
		  "content/img/flag/*.png"
	  ],
      "matches": [
        "*://*/*"
      ]
    }      
   ]
}