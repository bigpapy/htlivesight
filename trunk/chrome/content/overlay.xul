<?xml version="1.0"?><?xml-stylesheet href="chrome://htlivesight/content/overlay.css" type="text/css"?><?xml-stylesheet href="chrome://htlivesight/content/button/button.css" type="text/css"?><!DOCTYPE window SYSTEM "chrome://htlivesight/locale/overlay.dtd"><overlay id="htlivesight_overlay"         xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"><!-- added prefs part for adding button on install  -->        <script type="application/x-javascript" src="./prefs.js" /> <!-- end added prefs part for adding button on install --><script type="application/x-javascript" src="overlay.js" /><script type="application/x-javascript" src="log.js" /><script type="application/x-javascript" src="./button/button.js" /><!-- install button added by bigpapy --><!-- load listeners --><script type="application/x-javascript"><![CDATA[	window.addEventListener("load",		function() { htlivesight.installButton(); }, false);	]]></script><!-- end added part for install button --><!-- Firefox --><toolbarpalette id="BrowserToolbarPalette">  <toolbarbutton id="htls-button-1"/>    </toolbarpalette><!-- button details --><toolbarbutton id="htls-button-1"  insertbefore="print-button"    label="HTLiveSight"    tooltiptext="Click to open HTLiveSight"    class="toolbarbutton-1"    type="menu"    context="HTLS-menu">    <menupopup id="HTLS-menu">  		<menuitem id="Preferences" label="Options" oncommand="htls[3]();"/>				<menuitem id="Start HTLS" label="Start" oncommand="htls[2]();" />			</menupopup>	</toolbarbutton>  <menupopup id="menu_ToolsPopup">   <menu id="hattricktools_menu" label="Hattrick">    <menupopup id="hattricktools_menupopup">      <menuitem id="htlivesight_menuitem"      label="&menuitem.value;"      oncommand="htlivesight.onMenuItemCommand(event);" />    </menupopup>  </menu></menupopup></overlay>